var {{{streamName}}};
(function() {
  var $element = {{{seletingCode}}};
  var type = $element.prop("type");
  var tagName = $element.prop("tagName");
  if(tagName === "INPUT") {
    switch(type) {
      case "text":
        {{{streamName}}} = Bacon.$.textFieldValue($element);
        break;
      case "checkbox":
        {{{streamName}}} = Bacon.$.checkBoxValue($element);
        break;
      case "select":
        {{{streamName}}} = Bacon.$.selectValue($element);
        break;
      default:
        throw "input type \"" + type + "\" is not supported for RAttribute.";
    }
    {{{streamName}}}.addSource({{{signalStreamName}}});
  }
  else {
    throw "tag \"" + tagName + "\" is not supported for RAttribute.";
  }
})();